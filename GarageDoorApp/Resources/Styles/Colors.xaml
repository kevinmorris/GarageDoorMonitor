<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Black">Black</Color>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}"/>

    <Color x:Key="White">White</Color>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>

    <Color x:Key="GrayLight">#BBBBBB</Color>
    <SolidColorBrush x:Key="GrayLightBrush" Color="{StaticResource GrayLight}"/>

    <Color x:Key="GrayDark">#404040</Color>
    <SolidColorBrush x:Key="GrayDarkBrush" Color="{StaticResource GrayDark}"/>

    <Color x:Key="Red">#F03B3B</Color>
    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource Red}"/>

    <Color x:Key="Green">#4BAA32</Color>
    <SolidColorBrush x:Key="GreenBrush" Color="{StaticResource Green}"/>

</ResourceDictionary>